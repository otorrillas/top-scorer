{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:lodash/recommended"
  ],
  "plugins": ["lodash", "react"],
  "overrides": [
    {
      "files": ["*.spec.js"],
      "rules": {}
    }
  ],
  "env": {
    "jest": true
  },
  "settings": {
    "react": {
      "pragma": "React",
      "version": "16.7"
    }
  },
  "globals": {
    "fetch": true,
    "process": true
  }
}
